{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\MSSQLLocalDB;Database=NavPlatform;Trusted_Connection=True;TrustServerCertificate=True"
  },
  "Jwt": {
    "Issuer": "http://localhost:7029",
    "Audience": "http://localhost:5294",
    "Key": "__READ_FROM_ENV__"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RabbitMq": {
    "Enabled": false,
    "HostName": "localhost",
    "ExchangeName": "nav-exchange",
    "UserName": "guest",
    "Password": "__READ_FROM_ENV__"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Formatting.Compact" ],
